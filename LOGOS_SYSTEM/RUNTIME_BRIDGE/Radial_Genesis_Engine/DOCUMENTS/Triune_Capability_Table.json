  "regions": {
    "R_E": {
      "condition": "max_axis = E AND E > 0.5",
      "interpretation": "Existential pressure dominates"
    },
    "R_G": {
      "condition": "max_axis = G AND G > 0.5",
      "interpretation": "Normative pressure dominates"
    },
    "R_T": {
      "condition": "max_axis = T AND T > 0.5",
      "interpretation": "Epistemic pressure dominates"
    },
    "R_ET": {
      "condition": "max_axis <= 0.5 AND min_axis < 0.2 AND G = min_axis",
      "interpretation": "Existential + epistemic, normative weak"
    },
    "R_EG": {
      "condition": "max_axis <= 0.5 AND min_axis < 0.2 AND T = min_axis",
      "interpretation": "Existential + normative, epistemic weak"
    },
    "R_GT": {
      "condition": "max_axis <= 0.5 AND min_axis < 0.2 AND E = min_axis",
      "interpretation": "Normative + epistemic, existential weak"
    },
    "R_B": {
      "condition": "max_axis <= 0.5 AND min_axis >= 0.2",
      "interpretation": "No axis dominates"
    }
  },
