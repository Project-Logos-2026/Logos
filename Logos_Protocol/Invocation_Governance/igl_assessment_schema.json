{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "InvocationAssessment",
  "type": "object",
  "required": [
    "protocol_id",
    "invocation_risk_class",
    "blocking_factors",
    "required_constraints",
    "rationale",
    "final_state"
  ],
  "properties": {
    "protocol_id": { "type": "string" },
    "invocation_risk_class": {
      "enum": [
        "DENIED",
        "THEORETICALLY_BOUNDABLE",
        "STRICTLY_CONSTRAINED",
        "UNSAFE_BY_COMPOSITION"
      ]
    },
    "blocking_factors": {
      "type": "array",
      "items": { "type": "string" }
    },
    "required_constraints": {
      "type": "array",
      "items": { "type": "string" }
    },
    "rationale": { "type": "string" },
    "final_state": { "const": "DISCARDED" }
  },
  "additionalProperties": false
}
